func _ready():
	GridController.update_grid_height(35)
	
	for c in get_children():
		GridController.insert(GridController.closest_grid_space(c.position), c)
		
	$InputPortal3.set_summon_dir(Constants.Direction.Up)
	$ElbowPipe.mirror()
	$ElbowPipe.rotate_cw()
	$ElbowPipe.rotate_cw()
	
	$ElbowPipe4.mirror()
