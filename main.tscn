	$ElbowPipe5.mirror()
	
	
	$InputPortal4.set_summon_dir(Constants.Direction.Left)
	$ElbowPipe2.rotate_cw()
	
	
	$InputPortal2.set_summon_dir(Constants.Direction.Up)
	$ElbowPipe3.rotate_cw()
	$ElbowPipe3.rotate_cw()
	
"

